#include <stdio.h>

void digonal_of_matrix(int ar[3][3]){
    printf("matrix is ");
    for(int i=0;i<3;i++){
       for(int j=0;j<3;j++){
           printf("%d ",ar[i][j]);
       }
       printf("\n");
    }
    printf("digonal element is: ");
      for(int i=0;i<3;i++){
       for(int j=0;j<3;j++){

        if(i==j)
        printf("%d ",ar[i][j]);
       }
    }
}

int main()
{
int matrix[3][3];

printf("Enter matrix element:");
for(int i=0;i<3;i++){
    for(int j=0;j<3;j++){
        scanf("%d",&matrix[i][i]);
    }
}

digonal_of_matrix(matrix);
    return 0;
}
